<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen>

    <string name="ga_appName">APP_NAME</string>
    <string name="ga_appVersion">APP_VERSION</string>
    <!--Remember to disable verbose log level for production -->
    <string name="ga_logLevel">verbose</string>
    <integer name="ga_dispatchPeriod">1000</integer>
    <bool name="ga_dryRun">false</bool>

    <integer name="ga_sessionTimeout">300</integer>

    <!-- Enable automatic Activity measurement -->
    <bool name="ga_autoActivityTracking">true</bool>

    <!-- Enable automatic crash measurement  -->
    <bool name="ga_reportUncaughtExceptions">true</bool>


    <!-- The screen names that will appear in reports -->

    <screenName name="com.priyankam.flashlight">
        MainActivity
    </screenName>


    <!--  The following value should be replaced with correct property id. -->
    <string name="ga_trackingId">UA-92819289-1</string>
</PreferenceScreen>